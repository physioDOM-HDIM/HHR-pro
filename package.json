{
  "name": "PhysioDOM-server",
  "preferGlobal": false,
  "version": "0.0.30",
  "author": "Telecom Sante <tech@telecomsante.com>",
  "contributors": [
    {
      "name": "Fabrice Le Coz",
      "email": "fabrice.lecoz@telecomsante.com"
    }
  ],
  "description": "A web portal for professionnal",
  "scripts": {
    "preinstall": "npm install ./install/logger-v1.1.0.tar.gz",
    "install": "bower install; node install/install.js",
    "start": "supervisor -w package.json,locales/en.json,locales/nl.json,locales/es.json,locales/fr.json,config.json,server/ -- server/server.js -c config.json 2>&1 > log/app.log",
    "test": "node ./test/index.js"
  },
  "main": "./server/server.js",
  "bin": {
    "restoreDB": "./bin/restoreDB",
    "backupDB": "./bin/backupDB"
  },
  "repository": {
    "type": "git",
    "url": "http://git.telecomsante.com/physiodom-hdim/physiodom"
  },
  "dependencies": {
    "MD5": "~1.2.1",
    "agenda": "^0.6.28",
    "commander": "~2.1.0",
    "cookies": "~0.5.0",
    "i18n-2": "~0.4.6",
    "jsonschema": "^1.0.0",
    "logger": "1.1.0",
    "moment": "~2.8.4",
    "mongodb": "~1.4.7",
    "restify": "2.6.3",
    "rsvp": "~3.0.9",
    "soap": "^0.8.0",
    "swig": "~1.4.2",
    "tar.gz": "~0.1.1"
  },
  "devDependencies": {
    "chai": "^1.9.2",
    "grunt": "^0.4.5",
    "grunt-mocha": "^0.4.11",
    "grunt-mocha-test": "^0.12.1",
    "intern": "^2.2.2",
    "mocha": "^1.21.5",
    "request": "^2.45.0"
  },
  "jshintConfig": {
    "predef": [
      "before",
      "after",
      "it",
      "describe",
      "beforeEach",
      "afterEach"
    ],
    "nonew": true,
    "validthis": true,
    "curly": true,
    "latedef": true,
    "unused": "vars",
    "noarg": true,
    "forin": true,
    "noempty": true,
    "node": true,
    "eqeqeq": true,
    "strict": true,
    "undef": true,
    "bitwise": false,
    "newcap": true,
    "browser": true,
    "camelcase": true,
    "expr": true
  }
}
