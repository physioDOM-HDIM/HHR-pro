{
  "name": "PhysioDOM-server",
  "preferGlobal": false,
  "version": "0.0.1",
  "author": "Telecom Sante <tech@telecomsante.com>",
  "contributors": [
    {
      "name": "Fabrice Le Coz",
      "email": "fabrice.lecoz@telecomsante.com"
    }
  ],
  "description": "A web portal for professionnal",
  "scripts": {
    "install": "cd static; bower install; cd ..; sudo node install/install.js",
    "start": "supervisor -w package.json,server/ -- server/server.js -p 8001 2>&1 > log/app.log"
  },
  "main": "./server/server.js",
  "repository": {
    "type": "git",
    "url": "http://git.telecomsante.com/physiodom-hdim/physiodom"
  },
  "dependencies": {
    "commander": "2.1.0",
    "cookies": "~0.5.0",
    "logger": "git+ssh://git@git.telecomsante.com:tools/logger.git#v1.1.0",
    "moment": "2.5.1",
    "mongodb": "~1.4.7",
    "restify": "2.6.3",
    "rsvp": "3.0.9"
  },
  "devDependencies": {
    "chai": "^1.9.2",
    "mocha": "1.13.0",
    "request": "^2.45.0"
  },
  "jshintConfig": {
    "nonew":true, 
    "validthis":true, 
    "curly":true, 
    "latedef":true, 
    "unused":"vars", 
    "noarg":true, 
    "forin":true, 
    "noempty":true, 
    "node":true, 
    "eqeqeq":true, 
    "strict":true, 
    "undef":true, 
    "bitwise":true, 
    "newcap":true, 
    "browser":true, 
    "camelcase":true
  }
}
